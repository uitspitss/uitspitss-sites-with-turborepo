{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "$NODE_ENV",
    "$SG_API_KEY",
    "$SG_CONTACT_TEMPLATE_ID",
    "$NEXT_PUBLIC_GA_MEASUREMENT_ID"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
