{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "storybook": {},
    "i18next-scanner": {}
  },
  "globalEnv": [
    "NODE_ENV",
    "SG_API_KEY",
    "SG_CONTACT_TEMPLATE_ID",
    "NEXT_PUBLIC_GA_MEASUREMENT_ID"
  ]
}
