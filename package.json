{
  "name": "uitspitss-sites-turbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:portfolio": "turbo run dev --parallel --filter=portfolio",
    "dev:otoge": "turbo run dev --parallel --filter=otoge-bookmark-*",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch --parallel",
    "storybook": "turbo run storybook",
    "storybook:portfolio": "turbo run storybook --parallel --filter=ui --filter=portfolio",
    "storybook:otoge": "turbo run storybook --parallel --filter=ui --filter=otoge-bookmark-frontend",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,mdx}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "latest",
    "turbo": "^1.5.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "resolutions": {
    "glob-parent": "^6.0.0",
    "trim": "^1.0.0",
    "trim-newlines": "^4.0.0",
    "loader-utils": "^2.0.4"
  },
  "packageManager": "yarn@1.22.19"
}
